{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": false,
        "declaration": false,
        "removeComments": false,
        "noImplicitAny": true,
        "outDir": "./built-tsc",
        "typeRoots": [ "node_modules/@types" ],
        "types": [
            "node"
        ]
    },
    "exclude": [
        "node_modules",
        "built-tsc"
    ]
}